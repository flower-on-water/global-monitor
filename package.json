{
  "name": "global-monitor",
  "author": "Alexander Galaydyuk",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "gcz": "HUSKY_SKIP_HOOKS=1 git-cz",
    "gczr": "HUSKY_SKIP_HOOKS=1 git-cz --retry",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:version": "HUSKY_SKIP_HOOKS=1 standard-version --no-verify --sign",
    "build:changelog": "conventional-changelog --config node_modules/cz-conventional-changelog angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "build:docs": "typedoc --out build_docs/ --readme ./README.md --excludeExternals --includeDeclarations",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "chalk": "^2.4.2",
    "core-js": "^3.2.1",
    "lodash-es": "^4.17.15",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuetify": "^2.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@types/jest": "^24.0.17",
    "@types/lodash-es": "^4.17.3",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/cli-shared-utils": "^3.10.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "electron": "6.0.1",
    "electron-builder": "^21.2.0",
    "eslint": "5.16.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "standard-version": "^7.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-electron-builder": "^1.3.6",
    "vue-cli-plugin-vuetify": "^0.6.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "webpack": "^4.39.1",
    "webpack-chain": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "workspaces": [
    "extensions/*"
  ]
}
